{
  "laxbreak":true,
    "strict": true, // требует, чтобы код был написан в соответствии с строгим режимом EcmaScript 5.
    "camelcase": true, // требует именование переменных в стиле CamelCase
    "undef": true, // запрещает использовать не объявленные переменные
    "latedef": true, // запрещает использовать переменную до ее объявления
    "eqeqeq": true, // запрещает использовать == и != в пользу === и !==
    "curly": true, // требует всегда тело условий и циклов заключать в фигурные скобки
    "newcap": true, // требует обязательное использование 'new' перед вызовом функций-конструкторов
    "freeze": true, // запрещает модифицировать свойства и методы в прототипах нативных объектов (например Array, Date)
    "globals": { // объект разрешенных глобальных переменных в проекте
        "define": true,
        "require": true,
	"requirejs": true 
    },
    "browser": true, // определяет глобальные объекты браузера (document, location и.т.п)
    "devel": true, // определяет глобальные объекты для дебага (console, debugger и.т.п)
    "jquery": true // определяет глобальные объекты jQuery

}